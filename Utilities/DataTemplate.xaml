<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:tractorCompany.ViewModel"
                    xmlns:view="clr-namespace:tractorCompany.View">

    <DataTemplate DataType="{x:Type vm:AuthorizationVM}">
        <view:Authorization />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CategoryVM}">
        <view:Category />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TractorsVM}">
        <view:Tractors />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SpecialTechnicsVM}">
        <view:SpecialTechnics />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdditionalEquipmentVM}">
        <view:AdditionalEquipment />
    </DataTemplate>

</ResourceDictionary>